{
  "name": "cap-provider",
  "version": "1.0.0",
  "description": "CAP Provider API - exposes Products service",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "[production]": {
        "auth": "xsuaa"
      },
      "destinations": true
    },
    "build": {
      "target": "gen"
    }
  }
}
